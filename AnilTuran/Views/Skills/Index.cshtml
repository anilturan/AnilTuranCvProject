@model IEnumerable<AnilTuran.Models.Skill>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserInformationID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BeceriAdi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BeceriDerecesi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Aciklama)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserInformationID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BeceriAdi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BeceriDerecesi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Aciklama)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.SkillID }) |
                @Html.ActionLink("Details", "Details", new { id = item.SkillID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.SkillID })
            </td>
        </tr>
    }

</table>
